{
  "name": "jts-express-server",
  "version": "1.0.0",
  "description": "Express.js server with JTS (Janus Token System) authentication",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "keygen": "ts-node src/scripts/keygen.ts"
  },
  "keywords": [
    "express",
    "jts",
    "authentication",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.1",
    "jts-core": "^1.0.0",
    "dotenv": "^16.4.5",
    "cookie-parser": "^1.4.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.1",
    "@types/cookie-parser": "^1.4.7",
    "typescript": "^5.7.2",
    "ts-node-dev": "^2.0.0",
    "ts-node": "^10.9.2"
  }
}
